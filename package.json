{
  "name": "fis3-lint-rich-eslint",
  "version": "1.1.4",
  "description": "based on eslint and has rich configuration for fis3",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eslint",
    "fis3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoweiTang/fis3-lint-rich-eslint.git"
  },
  "bugs": {
    "url": "https://github.com/guoweiTang/fis3-lint-rich-eslint/issues"
  },
  "author": {
    "name": "ice.tang"
  },
  "license": "ISC",
  "eslintConfigs": {
    "envs": [
      "browser",
      "es6",
      "worker",
      "amd",
      "jquery"
    ],
    "globals": [
      "module",
      "exports",
      "__inline",
      "__uri",
      "__RESOURCE_MAP__",
      "fis"
    ],
    "useEslintrc": false,
    "rules": {
      "indent": [
        1,
        4
      ],
      "semi-spacing": [
        1
      ],
      "comma-spacing": [
        1
      ],
      "space-before-blocks": [
        1,
        "always"
      ],
      "space-before-function-paren": [
        1,
        "never"
      ],
      "space-infix-ops": [
        1
      ],
      "space-in-parens": [
        1
      ],
      "no-multi-spaces": [
        1
      ],
      "keyword-spacing": [
        1
      ],
      "key-spacing": [
        1
      ],
      "array-bracket-spacing": [
        1
      ],
      "object-curly-spacing": [
        1
      ],
      "spaced-comment": [
        1,
        "always",
        {
          "exceptions": [
            "-",
            "+",
            "*"
          ]
        }
      ],
      "for-direction": [
        2
      ],
      "no-await-in-loop": [
        2
      ],
      "no-prototype-builtins": [
        2
      ],
      "default-case": [
        2
      ],
      "eqeqeq": [
        2
      ],
      "no-loop-func": [
        2
      ],
      "no-unmodified-loop-condition": [
        2
      ],
      "require-await": [
        2
      ],
      "wrap-iife": [
        1,
        "inside"
      ],
      "semi": [
        1,
        "always"
      ],
      "guard-for-in": [
        2
      ],
      "no-unused-vars": [
        2,
        {
          "argsIgnorePattern": "^(require)|(exports)|(module)$"
        }
      ],
      "no-alert": [
        1
      ],
      "no-console": [
        0
      ],
      "no-caller": [
        2
      ],
      "no-eval": [
        1
      ],
      "no-with": [
        2
      ],
      "no-restricted-globals": [
        2
      ],
      "no-extend-native": [
        1
      ],
      "no-new-wrappers": [
        2
      ],
      "no-param-reassign": [
        2
      ],
      "no-magic-numbers": [
        1
      ],
      "no-throw-literal": [
        2
      ],
      "prefer-promise-reject-errors": [
        2
      ],
      "no-undef-init": [
        2
      ],
      "vars-on-top": [
        1
      ],
      "no-use-before-define": [
        2
      ],
      "no-undefined": [
        2
      ],
      "eol-last": [
        2,
        "always"
      ],
      "arrow-body-style": [
        1
      ],
      "arrow-parens": [
        1,
        "as-needed"
      ],
      "new-cap": [
        2
      ],
      "no-useless-escape": 0,
      "no-control-regex": 0
    }
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "dependencies": {
    "eslint": "^4.12.1"
  },
  "homepage": "https://github.com/guoweiTang/fis3-lint-rich-eslint#readme"
}